#ifndef __6D_MODULESYSTEM_H
#define __6D_MODULESYSTEM_H
#include "6D/Values"
#include "6D/Operations"
BEGIN_NAMESPACE_6D(Modulesystem)

NODET exportsQ(const char* names, ...);
NODET exportsFQ(const char* fmt, const char* names, ...);
#define exports(...) Modulesystem::exportsQ(""#__VA_ARGS__, __VA_ARGS__)
#define exportsf(fmt, ...) Modulesystem::exportsFQ(fmt, ""#__VA_ARGS__, __VA_ARGS__)
DECLARE_FFI_FN(Dispatch)
#define DEFINE_MODULE(modname, exp) NODET modname = CLOSED_FFI_FN_NO_GC(Modulesystem::Dispatch, (exp));
END_NAMESPACE_6D(Modulesystem)
#endif /* ndef __6D_MODULESYSTEM_H */
