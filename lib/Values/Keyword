#ifndef __VALUES_KEYWORD_H
#define __VALUES_KEYWORD_H
#include <string>
#include "6D/Values"
#include "Values/Values"

BEGIN_NAMESPACE_6D(Values)
struct Keyword : Node {
private:
	std::string text;
	friend const char* getKeyword1Name(NodeT node);
public:
	Keyword(const char* name) {
		this->text = text;
	}
	virtual void str(FILE* destination) const;
};
END_NAMESPACE_6D(Values)

#endif /* ndef __VALUES_KEYWORD_H */
