	Ratio* ratio(Values::NodeT aa, Values::NodeT bb);
	bool ratioP(Values::NodeT n);
	Values::NodeT getRatioA(Values::NodeT n) G_5D_PURE;
	Values::NodeT getRatioB(Values::NodeT n) G_5D_PURE;
DECLARE_FFI_FN(RatioMaker)
DECLARE_FFI_FN(RatioP)
